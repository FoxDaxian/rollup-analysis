{
  "{src,bin,browser,typings}/**/*.ts": [
    "prettier --write",
    "tslint --project . --fix",
    "git add"
  ],
  "test/**/*.ts": [
    "prettier --write",
    "tslint --project test/typescript --fix",
    "git add"
  ],
  "{test/test,test/*/index,test/utils,test/**/_config}.js": [
    "prettier --write",
    "eslint --fix",
    "git add"
  ]
}
